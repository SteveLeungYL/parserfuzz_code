  }  open(FH, ">$ENV{MYSQL_TMP_DIR}/resource_group_vars.inc") or    die("Unable to open file: $!\n");
  print FH "let \$os_name= $osname;\n";
  print FH "let \$thr_prio_enable= $thr_prio_enable;\n";
  print FH "let \$num_vcpu= $num_vcpu;\n";
  close FH;
