delimiter //;
CREATE FUNCTION round_to_step(i INT, n INT) RETURNS INTBEGIN  DECLARE step INT;
  SELECT * INTO step FROM t_step;
  # We add 0.1 as a protection from inexact division.  RETURN FLOOR((i % (step * n) + 0.1) / step);
END//delimiter ;//SET @step3= @step * 3;
