perl;
    use strict;
    my $search_expression= $ENV{'SEARCH_EXPRESSION'}    or die "SEARCH_EXPRESSION not set";
    my $search_pattern=    $ENV{'SEARCH_PATTERN'}       or die "SEARCH_PATTERN not set";
    if ($search_expression =~ /$search_pattern/)    {        print "Pattern \"$search_pattern\" found\n";
    }    else    {        print "Pattern \"$search_pattern\" not found\n";
    }EOFCREATE TABLE t (c INT);
INSERT INTO t VALUES (1);
ANALYZE TABLE t;
SET debug = '+d,temptable_create_return_full';
SELECT * FROM (SELECT COUNT(*) FROM t GROUP BY c) as dt;
SET debug = '-d,temptable_create_return_full';
DROP TABLE t;
